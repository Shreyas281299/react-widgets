{
  "name": "@webex/widget-recents-demo",
  "description": "Webex React Recents Widget Demo",
  "main": "./cjs/index.js",
  "src": "./src/index.js",
  "module": "./es/index.js",
  "keywords": [],
  "author": "devsupport@webex.com",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/webex/react-widgets",
  "files": [
    "src",
    "dist",
    "cjs",
    "es"
  ],
  "version": "0.211.0",
  "scripts": {
    "build": "babel-node ../../../scripts/build/index.js dist widget-recents-demo",
    "build:src": "cross-env NODE_ENV=${NODE_ENV:-production} NODE_OPTIONS=--max-old-space-size=2048 yarn run build",
    "serve": "yarn run serve:src package widget-recents-demo",
    "serve:src": "cross-env NODE_ENV=${NODE_ENV:-development} babel-node ../../../scripts/start/index.js",
    "publish": "cross-env-shell NODE_ENV='' babel-node ../../../scripts/publish/index.js package widget-recents-demo"
  },
  "devDependencies": {
    "@babel/node": "^7.8.3",
    "cross-env": "^5.0.0"
  }
}
