{
  "roots": [
    "<rootDir>/packages/node_modules/"
  ],
  "testPathIgnorePatterns": ["<rootDir>/packages/@webex/redux-module-spaces/src/actions.test.js", "<rootDir>/packages/@webex/redux-module-users/src/actions.test.js", "<rootDir>/packages/@webex/redux-module-media/src/actions.test.js", "<rootDir>/packages/@webex/react-redux-spark/src/component.test.js"],
  "transformIgnorePatterns": ["<rootDir>/(?!@webex/sdk-component-adapter)", "<rootDir>/(?!swiper)"],
  "moduleNameMapper": {
    "^.+\\.(css|scss|less)$": "identity-obj-proxy",
    "^.+\\.(mp3|wav)$": "<rootDir>/__mocks__/sound-mock.js",
    "^.+\\.(gif|ttf|eot|svg|png|jpg)$": "<rootDir>/__mocks__/file-mock.js",
    "@ciscospark/react-(.*)": "<rootDir>/packages/@ciscospark/react-$1/src",
    "@ciscospark/redux-(.*)": "<rootDir>/packages/@ciscospark/redux-$1/src",
    "@ciscospark/widget-(.*)": "<rootDir>/packages/@ciscospark/widget-$1/src",
    "@ciscospark/spark-widget-base": "<rootDir>/packages/@ciscospark/spark-widget-base/src",
    "@webex/react-(.*)": "<rootDir>/packages/@webex/react-$1/src",
    "@webex/redux-(.*)": "<rootDir>/packages/@webex/redux-$1/src",
    "@webex/widget-(.*)": "<rootDir>/packages/@webex/widget-$1/src",
    "@webex/components": "<rootDir>/node_modules/@webex/components/dist/umd/webex-components.umd.js",
    "@webex/webex-widget-base": "<rootDir>/packages/@webex/webex-widget-base/src"
  },
  "testRegex": ".test.js$",
  "testURL": "http://localhost",
  "moduleDirectories": [
    "node_modules",
    "/packages/"
    "/packages/"
  ],
  "modulePaths": [
    "node_modules",
    "<rootDir>/@webex",
    "<rootDir>/packages"
  ],
  "haste": {
    "providesModuleNodeModules": [".*"]
  },
  "verbose": true,
  "setupFiles": [
    "<rootDir>/scripts/tests/jest-setup.js"
  ]
}
